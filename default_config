server {
	listen localhost:8000;
	server_name melee.42.fr;
	location /{
		autoindex on;
		root ./;
	}
}

server {
	listen localhost:8001;
	server_name webserv.def;
	client_max_body_size 1;
	location / {
		root ./;
		index index.html;
		allowed_methods DELETE;
		cgi_pass /cgi;
	}
	location /webpage{
		root ./;
		autoindex on;
		allowed_methods DELETE;
	}
	
}

